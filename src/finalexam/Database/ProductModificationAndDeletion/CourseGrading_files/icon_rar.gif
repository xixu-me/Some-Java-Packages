    <script>
        $(document).ready(function(){            
           cgfooterByResize();
        });        
        
        $(window).resize(function (){           
           cgfooterByResize();
        });
        function cgfooterByResize() {
           
            var cgclientNAVHeight = 60;
            var cgclientNAV = document.getElementById('cgclientNAV');
            if (typeof(cgclientNAV) != 'undefined' && cgclientNAV != null) {  
                cgclientNAVHeight = document.getElementById('cgclientNAV').clientHeight;
            }
            
            var cgcontainerID =  document.getElementById('cgcontainerID');
            if (typeof(cgcontainerID) != 'undefined' && cgcontainerID != null) {                
                var cgPageBodyHeight = document.getElementById('cgcontainerID').clientHeight;
                var cgFooterHeight = document.getElementById('cgfooter').clientHeight;   
                
                var winMinHeight =  window.innerHeight - cgFooterHeight - cgclientNAVHeight;
               
                $("#cgcontainerID").attr('style', "min-height:" + winMinHeight + "px");
                if(cgPageBodyHeight < winMinHeight){ 
                    $("#cgfooter").css("position","fixed");                           
                }else{
                    $("#cgfooter").css("position","relative");          
                }                
            }    
        }        
    </script>
    

